{
	"$schema": "https://inlang.com/schema/inlang-message-format",
	"dropzone_title": [
		{
			"declarations": [
				"input dragActive",
				"input mobile"
			],
			"selectors": [
				"dragActive",
				"mobile"
			],
			"match": {
				"dragActive=1, mobile=0": "Dateien hier ablegen",
				"dragActive=0, mobile=0": "Hier Dateien ablegen oder klicken, um sie hochzuladen",
				"dragActive=*, mobile=1": "Hier klicken, um Dateien hochzuladen"
			}
		}
	],
	"dropzone_subtitle": "Alle Dateitypen werden unterstützt!",
	"currently_uploading": "Aktueller Upload",
	"upload_queue_title": "Upload Warteschlange",
	"upload_queue_subtitle": [
		{
			"declarations": [
				"input fileCount",
				"input totalSize",
				"local fileCountPlural = fileCount: plural"
			],
			"selectors": [
				"fileCountPlural",
				"totalSize"
			],
			"match": {
				"fileCountPlural=other, totalSize=*": "{fileCount} Dateien, {totalSize} insg.",
				"fileCountPlural=one": "1 Datei, {totalSize}"
			}
		}
	],
	"clear_upload_queue": "Leeren",
	"upload_queue_empty": "Die Upload-Warteschlange ist leer.",
	"rtc_status": [
		{
			"declarations": [
				"input status"
			],
			"selectors": [
				"status"
			],
			"match": {
				"status=new": "Getrennt",
				"status=connecting": "Verbinde...",
				"status=connected": "Verbunden",
				"status=dataChannelOpen": "Datenkanal offen",
				"status=disconnected": "Getrennt",
				"status=failed": "Verbindunsfehler",
				"status=closed": "Verbindung beendet"
			}
		}
	],
	"ping": "Ping: {ping} ms",
	"currently_downloading": "Aktueller Download",
	"download_queue_title": "Download Warteschlange",
	"download_queue_subtitle": [
		{
			"declarations": [
				"input fileCount",
				"input totalSize",
				"local fileCountPlural = fileCount: plural"
			],
			"selectors": [
				"fileCountPlural",
				"totalSize"
			],
			"match": {
				"fileCountPlural=other, totalSize=*": "{fileCount} Dateien, {totalSize} insg.",
				"fileCountPlural=one": "1 Datei, {totalSize}"
			}
		}
	],
	"download_queue_empty": "Die Download-Warteschlange ist leer.",
	"share_url": "Link teilen",
	"share_url_text": "Teile diese URL mit anderen, damit sie die Dateien herunterladen können:"
}