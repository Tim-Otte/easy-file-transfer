{
	"$schema": "https://inlang.com/schema/inlang-message-format",
	"dropzone_title": [
		{
			"declarations": [
				"input dragActive",
				"input mobile"
			],
			"selectors": [
				"dragActive",
				"mobile"
			],
			"match": {
				"dragActive=1, mobile=0": "Drop files here",
				"dragActive=0, mobile=0": "Drag & drop files or click to upload",
				"dragActive=0, mobile=1": "Click here to upload files"
			}
		}
	],
	"dropzone_subtitle": "All file types are supported!",
	"currently_uploading": "Current upload",
	"upload_queue_title": "Upload queue",
	"upload_queue_subtitle": [
		{
			"declarations": [
				"input fileCount",
				"input totalSize",
				"local fileCountPlural = fileCount: plural"
			],
			"selectors": [
				"fileCountPlural",
				"totalSize"
			],
			"match": {
				"fileCountPlural=other, totalSize=*": "{fileCount} files, {totalSize} total",
				"fileCountPlural=one": "1 file, {totalSize}"
			}
		}
	],
	"clear_upload_queue": "Clear queue",
	"upload_queue_empty": "The upload queue is empty.",
	"rtc_status": [
		{
			"declarations": [
				"input status"
			],
			"selectors": [
				"status"
			],
			"match": {
				"status=new": "Disconnected",
				"status=connecting": "Connecting...",
				"status=connected": "Connected",
				"status=dataChannelOpen": "Data channel open",
				"status=disconnected": "Disconnected",
				"status=failed": "Connection failed",
				"status=closed": "Connection closed"
			}
		}
	],
	"ping": "{ping} ms",
	"p2p_ping_tooltip": "Ping P2P connection",
	"signaling_ping_tooltip": "Ping server connection",
	"currently_downloading": "Current download",
	"download_queue_title": "Download queue",
	"download_queue_subtitle": [
		{
			"declarations": [
				"input fileCount",
				"input totalSize",
				"local fileCountPlural = fileCount: plural"
			],
			"selectors": [
				"fileCountPlural",
				"totalSize"
			],
			"match": {
				"fileCountPlural=other, totalSize=*": "{fileCount} files, {totalSize} total",
				"fileCountPlural=one": "1 file, {totalSize}"
			}
		}
	],
	"download_queue_empty": "The download queue is empty.",
	"share_url": "Share URL",
	"share_url_text": "Share this URL with others to allow them to download the files:"
}